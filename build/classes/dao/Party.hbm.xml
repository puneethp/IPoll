<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao">
  <class name="Party">
  	<id name="partyname" type="string" column="partyname">
  		<generator class="assigned" />
  	</id>
  	<property name="head" column="head" not-null="true" type="string" />
  	<property name="symbol" column="symbol" not-null="true" type="string" />
	<property name="validity" column="validity" not-null="true" />
  </class>
</hibernate-mapping>